{
  "manifest_version": 2,
  "name": "Reduxify",
  "description": "Redux dev tool",
  "version": "1.0",
  "author": "Ammad Kanwi, Michael Lampe, Stephenie Reimer",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "./popup/popup.html",
    "default_title": "Reduxify"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "storage"
  ],
   "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["content/script.js"]
    }
  ]
}


